{
  "name": "all-knu-mobile-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -e .env.dev turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "dotenv-cli": "latest",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "next": "^13.2.1",
    "recoil": "^0.7.7",
    "vite": "^4.1.0"
  },
  "packageManager": "npm@8.19.2"
}
