{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*dev"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "public/dist/**", "storybook-static/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "move": {
      "cache": false
    }
  }
}
